{
    "name": "omarrida/laravel-scribe",
    "description": "Spend less time worrying about maintaining docs and more time writing great code.",
    "keywords": [
        "scribe",
        "laravel",
        "api",
        "docs",
        "generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Omar Rida",
            "email": "omarrida94@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "erusev/parsedown": "^1.7",
        "fzaninotto/faker": "^1.9",
        "illuminate/console": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/validation": "^8.0",
        "kitetail/zttp": "^0.6.0",
        "laravel/helpers": "^1.1",
        "tymon/jwt-auth": "1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Omarrida\\Scribe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omarrida\\Scribe\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omarrida\\Scribe\\ScribeServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
